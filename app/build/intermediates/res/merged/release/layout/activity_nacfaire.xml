<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fr.elegary.nacfairesoundbox.Nacfaire">


<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
       <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:gravity="center"
           >
       <Button
           android:layout_width="100dp"
           android:layout_height="100dp"
           android:text="Séisme"
           android:layout_marginTop="0dp"

           android:id="@+id/btn_seisme"
           />

              <Button
                  android:id="@+id/btn_monsieur"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Monsieur" />

              <Button
                  android:id="@+id/btn_chut"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginLeft="25dp"
                  android:layout_marginTop="0dp"
                  android:text="Chut" />
       </LinearLayout>

       <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:gravity="center">
              <Button
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:text="Bassin doux"
                  android:layout_marginTop="0dp"
                  android:id="@+id/btn_bassin_doux"
                  />

              <Button
                  android:id="@+id/btn_bilan_rouge"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Bilan rouge" />
              <Button
                  android:id="@+id/btn_take_out_svt"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Take out" />
       </LinearLayout>
       <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:gravity="center">
              <Button
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:text="Tectoniques"
                  android:layout_marginTop="0dp"
                  android:id="@+id/btn_plaques_tectoniques"
                  />

              <Button
                  android:id="@+id/btn_issou"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="ISSOU" />
              <Button
                  android:id="@+id/btn_meiose"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Meiose" />
       </LinearLayout>
       <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:gravity="center">
              <Button
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:text="Tant pis"
                  android:layout_marginTop="0dp"
                  android:id="@+id/btn_tant_pis"
                  />

              <Button
                  android:id="@+id/btn_c_pas_sorcier"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="C PAS SORCIER" />
              <Button
                  android:id="@+id/btn_cahiers_svt"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Cahiers SVT" />
       </LinearLayout>
       <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:gravity="center">
              <Button
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:text="Cellules"
                  android:layout_marginTop="0dp"
                  android:id="@+id/btn_cellules"
                  />

              <Button
                  android:id="@+id/btn_comment_fonctionne"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Comment" />
              <Button
                  android:id="@+id/btn_on_appelle"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="On appelle" />
       </LinearLayout>
       <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:gravity="center">
              <Button
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:text="Ta gueule"
                  android:layout_marginTop="0dp"
                  android:id="@+id/btn_ta_gueule"
                  />

              <Button
                  android:id="@+id/btn_fortnite"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Fortnite" />
              <Button
                  android:id="@+id/btn_mademoiselle"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Mademoiselle" />
       </LinearLayout>
       <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:gravity="center">
              <Button
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:text="DAB"
                  android:layout_marginTop="0dp"
                  android:id="@+id/btn_dab"
                  />

              <Button
                  android:id="@+id/btn_passer_rangs"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Rangs" />
              <Button
                  android:id="@+id/btn_planeur_meilleur"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Planeur" />
       </LinearLayout>
       <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:gravity="center">
              <Button
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:text="Problématique"
                  android:layout_marginTop="0dp"
                  android:id="@+id/btn_problematique"
                  />

              <Button
                  android:id="@+id/btn_schema_note"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Schema noté" />
              <Button
                  android:id="@+id/btn_trotinne"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="On trotinne" />
       </LinearLayout>
       <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:gravity="center">
              <Button
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:text="Un point"
                  android:layout_marginTop="0dp"
                  android:id="@+id/btn_un_point"
                  />

              <Button
                  android:id="@+id/btn_velo"
                  android:layout_width="100dp"
                  android:layout_height="100dp"
                  android:layout_marginTop="0dp"
                  android:layout_marginLeft="25dp"
                  android:text="Vélo" />

       </LinearLayout>

</LinearLayout>
</ScrollView>

</RelativeLayout>
